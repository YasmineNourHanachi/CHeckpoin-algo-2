ALGORITHM Checkpoint Algo htab
 i : INTEGER:=0;
 j: INTEGER:=0;
[set1]:=INTEGER:=0;
[set2]:=INTEGER:=0
 arr : ARRAY_OF INTEGER[];
 htab : HASH_TABLE<Key_type,Value_type>;
 count : INTEGER:=0;
BEGIN
    FOR i FROM 0 TO set1.length -1 STEP 1  DO
       htab.insert(set1[i],1)
         END_FOR
FOR j FROM 0 TO set2.length -1 STEP 1  DO
    IF (htab.lookup(set1[i])) THEN
        count:=htab.get(set1[i]+1)  ELSE
        htab.insert(set2[j],1)
    END_IF
END_FOR
END

