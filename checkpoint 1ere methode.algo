ALGORITHM Checkpoint ALGO
VAR
 sum: INTEGER:=0;
 i : INTEGER:=0;
 j: INTEGER:=0;
[set1]:=INTEGER:=0;
[set2]:=INTEGER:=0;
BEGIN
    FOR i FROM 0 TO set1.length-1 DO
    FOR j FROM 0 TO set2.length-1 DO
     IF (set1[i]<>set2[j]) 
     THEN
         sum = sum + set1[i]+set2[j];
    END_IF
    END_FOR    
    FOR i FROM 0 TO set1.length-1 DO
    FOR j FROM 0 TO set2.length-1  DO
    IF (set1[i]==set2[j])
     THEN
         sum = sum + set1[i]+set2[j];
    END_IF
    END_FOR

END
